#include <iostream>
using namespace std;

int main() {

    // Nagłówek kolumn
    cout << "   ";
    for (int i = 1; i <= 7; i++) {
        cout << i << " ";
    }
    cout << endl;

    // Linia oddzielająca
    cout << "  ";
    for (int i = 1; i <= 15; i++) {
        cout << "=";
    }
    cout << endl;

    // Główna tabliczka mnożenia
    for (int i = 1; i <= 7; i++) {
        cout << i << "|";  // numer wiersza i pionowa kreska

        for (int j = 1; j <= 7; j++) {
            int wynik = i * j;

            // proste wyrównanie: jeśli wynik < 10, dodaj spację
            if (wynik < 10)
                cout << " " << wynik << " ";
            else
                cout << wynik << " ";
        }
        cout << endl;
    }

    return 0;
}
