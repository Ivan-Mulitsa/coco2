#include <iostream>
using namespace std;

int main() {

for (int i=0;i<=7;i++){
	cout<<" "<<i;
}
cout<<endl;
for (int i=0;i<=7*3+2;i++){
	cout<<"=";
}
cout<<endl;
for (int i=1;i<=7;i++){
	
	 for (int n = 1; n <= 7; n++) {
            cout << i * n;
    
	cout<<i<<"|"<<endl;
}
}
   

    return 0;
}
